{
  "name": "trello_app",
  "version": "1.0.0",
  "author": "Vova",
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "scripts": {
    "hot": "webpack-dev-server --content-base --hot --inline",
    "build": "webpack --watch",
    "start": "nodemon --exec babel-node server.js",
    "startDev": "webpack-dev-server --mode development",
    "build-css": "node-sass --include-path scss assets/scss/style.scss assets/css/style.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "react-redux": "^5.0.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-hot-middleware": "^2.24.0"
  },
  "main": "bundle.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeaZerMe/trelloCopy.git"
  },
  "keywords": [],
